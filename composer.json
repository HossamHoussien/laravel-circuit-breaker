{
    "name": "hossamhoussien/laravel-circuit-breaker",
    "description": "This is my package laravel-circuit-breaker",
    "keywords": [
        "php",
        "laravel",
        "circuit-breaker",
        "php-circuit-breaker",
        "laravel-circuit-breaker"
    ],
    "homepage": "https://github.com/hossamhoussien/laravel-circuit-breaker",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Hossam Houssien",
            "email": "hossam.houssien.a@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "autoload": {
        "psr-4": {
            "HossamHoussien\\CircuitBreaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HossamHoussien\\CircuitBreaker\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HossamHoussien\\CircuitBreaker\\CircuitBreakerServiceProvider"
            ],
            "aliases": {
                "CircuitBreaker": "HossamHoussien\\CircuitBreaker\\Facades\\CircuitBreaker"
            }
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "pest": "vendor/bin/pest"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "nunomaduro/collision": "^6.4",
        "orchestra/testbench": "^7.55",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin": "^1.1",
        "pestphp/pest-plugin-laravel": "^1.4"
    }
}
